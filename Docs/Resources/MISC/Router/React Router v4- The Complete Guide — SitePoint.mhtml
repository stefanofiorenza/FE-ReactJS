<span class=3D"token keyword">const</span> rob <span class=3D"token operato=
r">=3D</span> me<span class=3D"token punctuation">;</span>

rob<span class=3D"token punctuation">.</span>name <span class=3D"token oper=
ator">=3D</span> <span class=3D"token string">'Rob'</span><span class=3D"to=
ken punctuation">;</span>

<span class=3D"token builtin">console</span><span class=3D"token punctuatio=
n">.</span><span class=3D"token function">log</span><span class=3D"token pu=
